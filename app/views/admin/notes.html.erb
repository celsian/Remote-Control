<div class="row">
  <div class="col-md-12">
    <div class="panel panel-default">
      <!-- Default panel contents -->
      <div class="panel-heading"><h3 class="panel-title">Action History</h3></div>

      <!-- Table -->
      <table class="table table-striped">
          <thead>
            <tr>
              <th>Date:</th><th>Action:</th>
            </tr>
          </thead>
        <% @notes.each do |note| %>
          <tr>
            <td><%= note.created_at.strftime("%H:%M:%S on %m-%d-%Y") %></td><td><%= note.info %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>